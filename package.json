{
  "name": "webpack-boilerplate",
  "version": "1.0.1",
  "description": "webpack-boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "env-cmd -f ./env/.env_dev webpack serve --config config/webpack.dev.js",
    "dev_server": "env-cmd -f ./env/.env_dev_test_server webpack serve --config config/webpack.dev.js",
    "build": "env-cmd -f ./env/.env_prod  webpack --config config/webpack.prod.js",
    "build_extract": "env-cmd -f ./env/.env_prod  webpack --config config/webpack.prod.extract.js"
